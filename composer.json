{
    "name": "yiisoft/cache-file",
    "type": "library",
    "description": "Yii Caching Library - File Handler",
    "keywords": [
        "yii",
        "framework",
        "cache",
        "file",
        "psr-16"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/cache-file/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/cache-file"
    },
    "require": {
        "php": "^7.4|^8.0",
        "psr/simple-cache": "~1.0.0",
        "yiisoft/serializer": "dev-master"
    },
    "require-dev": {
        "php-mock/php-mock-phpunit": "^2.4",
        "infection/infection": "^0.20.0",
        "phpunit/phpunit": "^9.4",
        "vimeo/psalm": "^4.1",
        "yiisoft/aliases": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Cache\\File\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Cache\\File\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "common": "config/common.php",
            "params": "config/params.php"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
